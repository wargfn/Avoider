package{		/*	...	author:wargfn@wargfndev.blogspot.com	This is the MAIN Splash screen, just to see if	I can keep a roll going	*/		import flash.display.MovieClip;	import flash.events.MouseEvent;	import flash.display.SimpleButton;		public class TitleScreen extends MovieClip	{		public function TitleScreen()		{						startButton.addEventListener(MouseEvent.CLICK, onClickStart);					}				public function onClickStart(mouseEvent:MouseEvent):void		{						dispatchEvent(new NavigationEvent(NavigationEvent.START));					}			}	}